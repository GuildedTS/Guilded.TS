{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"isolatedModules": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
