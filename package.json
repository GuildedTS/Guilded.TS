{
	"name": "@guildedts/guilded.ts",
	"version": "0.0.0",
	"description": "A powerful NPM module that allows you to easily interact with the Guilded API.",
	"private": true,
	"scripts": {
		"test": "ts-node test",
		"build": "pnpm -r build",
		"lint": "eslint packages/**/*.ts --fix",
		"format": "prettier --write packages/**/*.ts",
		"docs": "rimraf docs && typedoc --options tsdoc.json",
		"prepare": "pnpm build",
		"clear": "pnpm -r clear",
		"refresh": "pnpm clear && rimraf node_modules && pnpm i",
		"changeset": "changeset",
		"version": "changeset version",
		"publish": "pnpm build && changeset publish"
	},
	"keywords": [
		"guilded",
		"api",
		"js",
		"ts",
		"types"
	],
	"contributors": [
		"Gamertike (https://gamertike.com)",
		"Bruno Lepis (https://brunolepis.xyz)",
		"vyPal"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/guildedts/guilded.ts.git"
	},
	"bugs": {
		"url": "https://github.com/guildedts/guilded.ts/issues"
	},
	"homepage": "https://guildedts.js.org",
	"license": "Apache-2.0",
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"@changesets/cli": "^2.24.0",
		"@types/glob": "^7.2.0",
		"@types/node": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.8",
		"typedoc-plugin-missing-exports": "^0.23.0",
		"typescript": "^4.7.4",
		"@guildedts/builders": "workspace:^",
		"@guildedts/framework": "workspace:^",
		"guilded-api-typings": "workspace:^",
		"guilded.ts": "workspace:^",
		"@guildedts/rest": "workspace:^",
		"@guildedts/ws": "workspace:^"
	},
	"packageManager": "pnpm@7.6.0"
}
