{
	"name": "@guildedts/guilded.ts",
	"version": "0.2.0",
	"description": "A powerful NPM module that allows you to easily interact with the Guilded API.",
	"private": true,
	"scripts": {
		"test": "ts-node example",
		"build": "yarn turbo run build",
		"lint": "yarn eslint 'packages/**/*.ts' --fix",
		"format": "yarn prettier --write 'packages/**/*.{ts,js,json}'",
		"docs": "yarn rimraf docs && yarn typedoc --options tsdoc.json && echo 'guildedts.js.org' > docs/CNAME",
		"prepare": "yarn format && yarn lint",
		"clear": "yarn turbo run clear",
		"refresh": "yarn clear && yarn rimraf node_modules && yarn install && yarn build",
		"setchanges": "yarn changeset",
		"version": "yarn setchanges version"
	},
	"keywords": [
		"guilded",
		"api",
		"js",
		"ts",
		"types"
	],
	"contributors": [
		"Gamertike <contact@gamertike.com> (https://gamertike.com)",
		"Bruno Lepis (https://brunolepis.xyz)"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/GuildedTS/Guilded.TS.git"
	},
	"bugs": {
		"url": "https://github.com/GuildedTS/Guilded.TS/issues"
	},
	"homepage": "https://guildedts.js.org",
	"license": "Apache-2.0",
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"@changesets/cli": "^2.22.0",
		"@types/node": "^17.0.27",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"ts-node": "^10.7.0",
		"turbo": "^1.2.5",
		"typedoc": "^0.22.15",
		"typedoc-plugin-missing-exports": "^0.22.6",
		"typescript": "^4.6.3"
	},
	"packageManager": "yarn@3.2.0"
}
